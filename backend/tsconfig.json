{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"lib": ["ES2020"],
		"rootDir": "src",
		"outDir": "dist",
		"strict": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"skipLibCheck": true
		// "paths": {
		// 	"@types/express": ["./node_modules/express"]
		// }
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
